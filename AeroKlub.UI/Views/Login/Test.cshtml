
@{
    Layout = null;
}


<!DOCTYPE html>

<html>
<head>
	<meta name="viewport" content="width=device-width" />
	<title>Test</title>


	<link href="~/css/nouislider.css" rel="stylesheet" />
	<script src="~/Scripts/nouislider.js"></script>
	<link href="~/css/nouislider.min.css" rel="stylesheet" />
	<script src="~/Scripts/nouislider.min.js"></script>
	<script src="~/Scripts/wNumb.js"></script>

	
</head>
<body>
	<div class="container-fluid">
		<div id='#1' style="width:250px; margin:100px 10px 70px 10px;" />
	</div>

	<script language="javascript">
		var html5Slider = document.getElementById('#1');

		noUiSlider.create(html5Slider, {
			connect: true,
			behaviour: 'tap',
			start: [2, 5],
			tooltips: [wNumb({ decimals: 0 }), wNumb({ decimals: 0 })],
			range: {
				'min': [0, 1],
				'max': [24]
			},
			pips: {
				mode: 'values',
				values: [0, 6, 12, 18, 24],
				density: 4
			},
			cssPrefix: 'noUi-',
			cssClasses: {

				target: 'target',
				base: 'Penis',
				origin: 'origin',
				handle: 'handle',
				handleLower: 'handle-lower',
				handleUpper: 'handle-upper',
				horizontal: 'horizontal',
				vertical: 'vertical',
				background: 'background',
				connect: 'connect',
				connects: 'connects',
				ltr: 'ltr',
				rtl: 'rtl',
				draggable: 'draggable',
				drag: 'state-drag',
				tap: 'state-tap',
				active: 'active',
				tooltip: 'tooltip',
				pips: 'pips',
				pipsHorizontal: 'pips-horizontal',
				pipsVertical: 'pips-vertical',
				marker: 'marker',
				markerHorizontal: 'marker-horizontal',
				markerVertical: 'marker-vertical',
				markerNormal: 'marker-normal',
				markerLarge: 'marker-large',
				markerSub: 'marker-sub',
				value: 'value',
				valueHorizontal: 'value-horizontal',
				valueVertical: 'value-vertical',
				valueNormal: 'value-normal',
				valueLarge: 'value-large',
				valueSub: 'value-sub'

			}

		});

	</script>

	div class="container-fluid">
	<div id='#2' style="width:250px; margin:100px 10px 70px 10px;" />


	<script language="javascript">
		var html5Slider = document.getElementById('#2');

		noUiSlider.create(html5Slider, {
			connect: true,
			behaviour: 'tap',
			start: [2, 5],
			tooltips: [wNumb({ decimals: 0 }), wNumb({ decimals: 0 })],
			range: {
				'min': [0, 1],
				'max': [24]
			},
			pips: {
				mode: 'values',
				values: [0, 6, 12, 18, 24],
				density: 4
			},
			cssPrefix: 'noUi-',
			cssClasses: {

				target: 'target',
				base: 'Penis1',
				origin: 'origin',
				handle: 'handle',
				handleLower: 'handle-lower',
				handleUpper: 'handle-upper',
				horizontal: 'horizontal',
				vertical: 'vertical',
				background: 'background',
				connect: 'connect',
				connects: 'connects',
				ltr: 'ltr',
				rtl: 'rtl',
				draggable: 'draggable',
				drag: 'state-drag',
				tap: 'state-tap',
				active: 'active',
				tooltip: 'tooltip',
				pips: 'pips',
				pipsHorizontal: 'pips-horizontal',
				pipsVertical: 'pips-vertical',
				marker: 'marker',
				markerHorizontal: 'marker-horizontal',
				markerVertical: 'marker-vertical',
				markerNormal: 'marker-normal',
				markerLarge: 'marker-large',
				markerSub: 'marker-sub',
				value: 'value',
				valueHorizontal: 'value-horizontal',
				valueVertical: 'value-vertical',
				valueNormal: 'value-normal',
				valueLarge: 'value-large',
				valueSub: 'value-sub'

			}

		});

	</script>


	<style>
		.noUi-Penis {
			width: 100%;
			height: 100%;
			position: relative;
			z-index: 1;
			background: rgba(0,0,0,0);
			background: linear-gradient(90deg, rgba(0,0,0,0) 41.16%, rgba(255,132,0,1) 41.16%, rgba(255,132,0,1) 83.2%, rgba(0,0,0,0) 83.2%);
		}

		.noUi-Penis1 {
			width: 100%;
			height: 100%;
			position: relative;
			z-index: 1;
			background-color: red;
		}
	</style>

</body>
</html>
